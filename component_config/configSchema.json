{
  "type": "object",
  "title": "extractor configuration",
  "required": [
    "address",
    "#api_key"
  ],
  "properties": {
    "address": {
      "type": "string",
      "title": "Address",
      "minLength": 42,
      "maxLength": 42,
      "description": "A valid Ethereum address you want to get data for including the 0x prefix.",
      "propertyOrder": 1
    },
    "start_block": {
      "type": "number",
      "title": "Start Block",
      "default": 0,
      "description": "Filter for starting block.",
      "propertyOrder": 2
    },
    "end_block": {
      "type": "number",
      "title": "End Block",
      "default": 99999999,
      "description": "Filter for ending block.",
      "propertyOrder": 3
    },
    "page": {
      "type": "number",
      "title": "Page",
      "default": 1,
      "description": "Page number of the results.",
      "propertyOrder": 4
    },
    "offset": {
      "type": "number",
      "title": "Offset",
      "default": 100,
      "description": "How many transactions per page.",
      "propertyOrder": 5
    },
    "sort": {
      "type": "string",
      "title": "Sort Type",
      "default": "asc",
      "description": "Sort transactions by timestamp in either ascending 'asc' or descending 'desc' order.",
      "propertyOrder": 6
    },
    "#api_key": {
      "type": "string",
      "format": "password",
      "title": "Etherscan API Key",
      "description": "Your etherscan API key. Can be found in the admin section of your Etherscan account.",
      "links": [
        {
          "rel": "Etherscan",
          "href": "https://etherscan.io/myapikey"
        }
      ],
      "propertyOrder": 7
    },
    "debug": {
      "type": "boolean",
      "title": "Debug",
      "propertyOrder": 8
    }
  }
}